{
  "name": "battleye",
  "version": "0.1.1",
  "description": "Battleye rcon client built in nodejs.",
  "main": "dist/lib/battleye/index.js",
  "repository": "https://github.com/nurdism/battleye",
  "homepage": "https://github.com/nurdism/battleye",
  "scripts": {
    "dev": "node --trace-warnings ./node_modules/nodemon/bin/nodemon --exec babel-node ./src/test/index.js",
    "build": "babel ./src/lib -d ./dist/lib",
    "release": "npm run lint && npm run build && standard-version && git push --follow-tags && npm publish",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "npm run lint -- --fix",
    "deps": "npm-check -u",
    "snyk": "snyk test"
  },
  "author": {
    "name": "Nurdism",
    "email": "nurdism.io@gmail.com",
    "url": "https://github.com/nurdism"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.4",
    "npm-check": "^5.8.0",
    "snyk": "^1.95.3",
    "standard-version": "^4.4.0"
  }
}
